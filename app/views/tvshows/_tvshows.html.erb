
<% @tvshows.shows.each do |show| %>
<% if show['show']["image"].present? %>
<%= image_tag(show['show']['image']['medium']) %>
<%= show['show']['name'] %>
<%= show['airdate'] %>
<%= show['season'] %>
<%= show['number'] %>
<%= show['show']['genres']%>
<%= link_to "Details",tvshow_path(show["show"]["id"]) %>
<% end %>
<% end %>
